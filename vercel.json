{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/sw.js",
      "dest": "/sw.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/scc-logo-.*\\.png",
      "dest": "/scc-logo-72.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_GEMINI_API_KEY": "@gemini_api_key",
    "VITE_GOOGLE_MAPS_API_KEY": "@google_maps_api_key",
    "VITE_API_URL": "@api_url"
  }
}
